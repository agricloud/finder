                    "    failure: function(form,action) {",
                    "        Ext.MessageBox.alert('Failure',action.result.message);",
                    "        that.getBatchViewer().down('button[itemId=commonDeleteBtn]').setDisabled(false);",
                    "    }",
                    "});"
